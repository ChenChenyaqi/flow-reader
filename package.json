{
  "name": "flow-reader",
  "version": "1.0.1",
  "description": "FlowReader - 让英文阅读像水流一样顺畅 | A Chrome extension for seamless English reading",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist/ && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test": "vitest",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "lucide-vue-next": "^0.562.0",
    "openai": "^6.15.0",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@crxjs/vite-plugin": "^2.3.0",
    "@tailwindcss/vite": "^4.1.10",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.23",
    "chrome-types": "^0.1.403",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "less": "^4.5.1",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "sharp": "^0.33.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  }
}
